{
	"name": "github-vis",
	"version": "0.0.0",
	"description": "The GitHub Visualizer",
	"private": true,
	"main": "index.html",
	"scripts": {
		"start": "webpack-dev-server --mode development --devtool source-map --output-filename index.js --content-base ../ --output-public-path /github-vis/www/ --open-page /github-vis/www",
		"build": "webpack --mode development --devtool source-map --output-filename index.js --output-path www/",
		"build-production": "webpack --mode production --output-filename index.js --output-path www/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fiddleplum/github-vis.git"
	},
	"author": "Stephen Hurley",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fiddleplum/github-vis/issues"
	},
	"homepage": "https://github.com/fiddleplum/github-vis",
	"dependencies": {
		"vue": "latest"
	},
	"devDependencies": {
		"eslint": "latest",
		"eslint-config-standard": "latest",
		"eslint-plugin-import": "latest",
		"eslint-plugin-node": "latest",
		"eslint-plugin-promise": "latest",
		"eslint-plugin-standard": "latest",
		"webpack": "latest",
		"webpack-cli": "latest",
		"webpack-dev-server": "latest"
	}
}
